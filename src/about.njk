<section class="section-mission" id="mission">
  <div class="mission-pin">
    <h2 class="mission-title">
      {% for item in page_about.mission.title %}
      <span class="mission-title__line {{ item.style }}">
        {{ item.text }}
      </span>
      {% endfor %}
    </h2>

    {% for i in range(1, page_about.mission.ripple_count + 1) %}
    <div class="mission-ripple mission-ripple-{{ loop.index }}"></div>
    {% endfor %}
  </div>
  <div class="mission-pin-steps pin-steps">
    <div class="mission-pin-step mission-pin-step-1 pin-step"></div>
    {% for item in page_about.mission.title %}
    <div
      class="mission-pin-step mission-pin-step-{{ loop.index + 1 }} pin-step"
    ></div>
    {% endfor %}
  </div>
</section>

{% if page_about.leadership.toggle_section %}
<section class="section-leadership section-gradient">
  <h2 class="section-leadership__title section-gradient__title">
    {{ page_about.leadership.section_title }}
  </h2>
  <p class="section-leadership__desc section-gradient__desc">
    {{ page_about.leadership.section_desc }}
  </p>

  <div class="leadership-portraits">
    <!-- prettier-ignore -->
    {% for img in page_about.leadership.list %}
      {% set src = img.src %}
      {% set name = img.name %}
      {% set role = img.role %}
      {% set class = '' %}
      {% if loop.index <= 3 %}
        {% set class = 'big-three' %}
      {% endif %}
      {% include "./_includes/components/portrait-gradient.njk" %}
    {% endfor %}
  </div>
</section>
{% endif %}
