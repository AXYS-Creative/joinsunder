<div class="cta-group">
  {% if cta_1_text and cta_1_url %}
  <a
    class="cta-1 {%if cta_1_outline%}outline{%endif%} tab-element-page"
    href="{{ cta_1_url }}"
    aria-label="{{ cta_1_text_description }}"
  >
    {{ cta_1_text }}
  </a>
  {% endif %}
  <!-- prettier-ignore -->
  {% if cta_2_text %}
  <a
    class="cta-2 {%if cta_2_outline%}outline{%endif%} tab-element-page"
    href="{{ cta_2_url }}"
  >
    {{ cta_2_text }}
  </a>
  {% endif %}
</div>
